<?xml version="1.0" encoding="UTF-8"?>

<phpunit>
	<testsuites>
		<testsuite name="Map">
			<directory>tests/</directory>
		</testsuite>
	</testsuites>
	<filter>
		<whitelist>
			<directory suffix=".php">src/</directory>
		</whitelist>
	</filter>
	<logging>
		<log type="coverage-clover" target="coverage.xml"/>
	</logging>
</phpunit>